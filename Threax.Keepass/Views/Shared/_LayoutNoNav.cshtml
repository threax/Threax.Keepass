@using Threax.AspNetCore.AuthCore;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KeePassWeb</title>
    <asset-bundle src="~/bundle.min.css"></asset-bundle>
    @RenderSection("css", required: false)
</head>
<body>
    <div class="container body-content mt-3">
        @RenderBody()
    </div>
    <div class="container">
        <hr />
    </div>

    <relogin></relogin>
    @* this line fills in the client config using the tag helper and what has been defined in the di *@
    <client-config csp-nonce></client-config>
    <token-config csp-nonce></token-config>
    <asset-bundle src="~/bundle.min.js"></asset-bundle>
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="~/lib/hr-run.js" asp-append-version="true"></script>
    <threax-csp></threax-csp>
</body>
</html>