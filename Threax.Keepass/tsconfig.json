{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": "obj/tscout/lib",
    "outFile": "wwwroot/lib/tsbin.js",
    "module": "amd",
    "baseUrl": ".",
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.iterable"
    ],
    "paths": {
      "clientlibs.*": [
        "Client\\Libs\\*"
      ],
      "hr.bootstrap.datetime.*": [
        "node_modules\\htmlrapier.bootstrap.datetime\\src\\*"
      ],
      "hr.bootstrap.*": [
        "node_modules\\htmlrapier.bootstrap\\src\\*"
      ],
      "hr.halcyon-explorer.*": [
        "node_modules\\htmlrapier.halcyon-explorer\\src\\*"
      ],
      "hr.halcyon.*": [
        "node_modules\\htmlrapier.halcyon\\src\\*"
      ],
      "hr.relogin.*": [
        "node_modules\\htmlrapier.relogin\\src\\*"
      ],
      "hr.roleclient.*": [
        "node_modules\\htmlrapier.roleclient\\src\\*"
      ],
      "hr.widgets.*": [
        "node_modules\\htmlrapier.widgets\\src\\*"
      ],
      "hr.*": [
        "node_modules\\htmlrapier\\src\\*"
      ]
    }
  },
  "compileOnSave": true,
  "include": [
    "Client\\Libs\\**\\*.ts",
    "Views\\**\\*.ts",
    "node_modules\\htmlrapier.bootstrap.datetime\\src\\**\\*.ts",
    "node_modules\\htmlrapier.bootstrap\\src\\**\\*.ts",
    "node_modules\\htmlrapier.form.bootstrap4\\**\\*.ts",
    "node_modules\\htmlrapier.halcyon-explorer\\src\\**\\*.ts",
    "node_modules\\htmlrapier.halcyon\\src\\**\\*.ts",
    "node_modules\\htmlrapier.relogin\\src\\**\\*.ts",
    "node_modules\\htmlrapier.roleclient\\src\\**\\*.ts",
    "node_modules\\htmlrapier.widgets\\src\\**\\*.ts",
    "node_modules\\htmlrapier\\src\\**\\*.ts"
  ],
  "exclude": [],
  "files": [
    "node_modules\\jsns\\jsns.ts"
  ]
}